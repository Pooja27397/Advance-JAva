package com.Revision;
//Find the second highest number in array
public class HighestNumSecond {
public static void main(String[] args) {
   int [] a= {6,8,2,5,4,7,9};
   int temp;
   int size=a.length;
   for(int i=0;i<size;i++) {
	   for(int j=i+1;j<size;j++) {
		   if(a[i]<a[j]) {
			   temp=a[i];
			   a[i]=a[j];
			   a[j]=temp;
		   }                                          
	   }
   }
	   System.out.println("second largetst number"+a[1]);
   }
}
   
