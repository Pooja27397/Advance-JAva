public class Add_Pra {
public static void main(String[] args) {
	System.out.println("Missing numbers are");
	int [] a= {1,3,4,5,7,8};
	int size=a.length;
(int num=1;num<=size;num++) {
	int count=0;
	for(int j=0;j<size;j++) {
		if(num==a[j]) {
			count++;
		}
	}
	
       if(count==0) {
    	   System.out.println(+num);
       }
}
       }
}