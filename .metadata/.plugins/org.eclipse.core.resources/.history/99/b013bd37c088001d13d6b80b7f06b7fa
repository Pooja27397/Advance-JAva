package com.Revision;

import java.util.Scanner;

public class Revise {
	public static void main(String[] args) {
	int[]a= {1,2,5,7,3,9,8};	
	int size=a.length;
		for(int i=0;i<size;i++) {
			for(int j=i+1;j<size;j++) {
				if(a[i]<a[j]) {
					int temp=a[i];
					a[i]=a[j];
					a[j]=temp;
				}
			}
		}
		System.out.println("Second Highest Number::"+a[1]);
	}
	}